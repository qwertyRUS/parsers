{
  "name": "parser-backend",
  "author": "sergeech",
  "version": "1.0.0",
  "main": "server.js",
  "license": "ISC",
  "description": "A backend parser with Node.js, Express, and Puppeteer",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "esbuild server.js --bundle --platform=node --target=node20 --outfile=dist/server.cjs",
    "lint": "eslint ."
  },
  "dependencies": {
    "2captcha": "^3.0.5-2",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "https-proxy-agent": "^7.0.6",
    "mongoose": "^8.16.1",
    "node-cron": "^4.2.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^22.10.0",
    "telegraf": "^4.16.3",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "esbuild": "^0.25.6",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  },
  "nodemon": {
    "watch": [
      "server.js",
      "src",
      "routes"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules",
      "mongo-init.js",
      "failed_captchas"
    ]
  }
}
